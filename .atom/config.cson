"*":
  "atom-ternjs":
    excludeLowerPriorityProviders: true
    useSnippets: true
  "autocomplete-plus":
    includeCompletionsFromAllBuffers: false
    strictMatching: true
  "bracket-matcher":
    autocompleteBrackets: false
    autocompleteSmartQuotes: false
  core:
    destroyEmptyPanes: false
    disabledPackages: [
      "autocomplete-snippets"
      "metrics"
      "minimap"
    ]
    openEmptyEditorOnStart: false
    themes: [
      "one-dark-ui"
      "gruvbox-plus-syntax"
    ]
    useCustomTitleBar: true
  editor:
    invisibles: {}
    scrollPastEnd: true
  linter:
    gutterPosition: "Left"
    showErrorInline: false
    showErrorPanel: false
    showErrorTabLine: true
  "linter-stylelint":
    disableWhenNoConfig: true
  tabs:
    addNewTabsAtEnd: true
    enableVcsColoring: true
  welcome:
    showOnStartup: false
  whitespace:
    ensureSingleTrailingNewline: false
